{
  "manifest_version": 3,
  "name": "Zoho asan",
  "version": "1.0",
  "description": "Zoho....... oho... aha ha ha haaaaa ha",
  "permissions": [
    "tabs",
    "activeTab",
    "scripting",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
    "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
